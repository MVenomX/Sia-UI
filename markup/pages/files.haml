#files
    .header
        .title File Manager
        .info
            .space 12.2 GB
            .cost 0.122 &micro;SC/h
            .costusd &asymp; 0.001 &micro;USD/h
    .cloud-control
        .row
            .col-xs-6
                .button.upload-public
                    %i.fa.fa-upload
                    Upload File
            .col-xs-6
                .search
                    &nbsp;
                    %i.fa.fa-search
    .upload-pane
        .path
            .stop
                %i.fa.fa-close
            .text
                %i.fa.fa-edit
                filename.txt
            .meta
                1.2 MB - 0.0122 SC/h
        .upload.button
            %i.fa.fa-upload
            Upload
        .public.button
            %i.fa.fa-check-square
            Public
        .account.button
            %i.fa.fa-sign-out
            Grocery Store Account
    .info-pane
        .name filename.txt
        .visibility Public
        .size 1.2 MB
        .cost 0.0122 SC/h
        .button.delete
            %i.fa.fa-close
            Delete
        .button.update
            %i.fa.fa-refresh
            Update
        .button.download
            %i.fa.fa-download
            Download
    .file-browser
